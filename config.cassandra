
Tables: 

CREATE TABLE lotus_dev.user(
   id uuid,
   nome varchar,
   email varchar,
   senha varchar,
   is_empresa varchar,
   inserted_at date,
   updated_at date,
   PRIMARY KEY (id)
   );

CREATE TABLE curriculo(
   id uuid PRIMARY KEY,
   file_base64 text,
   id_usuario varchar,
   descricao text,
   inserted_at timestamp,
   updated_at timestamp
  
   );
